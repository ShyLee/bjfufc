        	<field table="sc_zfbt" name="if_lxq"/>
        	<field table="sc_zfbt" name="money_lxq"/>
        	
        	<field table="sc_zfbt" name="date_apply" hidden="true"/>
        	<field table="sc_zfbt" name="date_approve" hidden="true"/>
        	<field table="sc_zfbt" name="date_pay_begin" hidden="true"/>
        	
        	<field table="sc_zfbt" name="po_name" hidden="true"/>
	        <field table="sc_zfbt" name="po_dv_name" hidden="true"/>
	        <field table="sc_zfbt" name="po_residence" hidden="true"/>   			
     </panel>
     
     
     <panel type="form" id="poInfoForm" showOnLoad="false" dataSource="zfBtDs" columns="3" layoutRegion="poInfoSite">
            <title translatable="true">po Info</title>
            <field table="sc_zfbt" name="id" hidden="true"/>
        	<field table="sc_zfbt" name="po_name" readOnly="true"/>
	        <field table="sc_zfbt" name="po_dv_name" readOnly="true"/>
	        <field table="sc_zfbt" name="po_residence"/>  			
     </panel>
     
