            <title translatable="true">Em Info</title>
            <field table="sc_zfbt" name="id" hidden="true"/>
            <field table="sc_zfbt" name="em_name" readOnly="true"/>
	        <field table="sc_zfbt" name="em_sex" readOnly="true"/>
	        <field table="sc_zfbt" name="em_id" readOnly="true"/>
            <field table="sc_zfbt" name="salary_num" readOnly="true"/>
	        <field table="sc_zfbt" name="zhiw_name" readOnly="true"/>
	        <field table="sc_zfbt" name="zhic_name" readOnly="true"/>
